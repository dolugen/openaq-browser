{
  "name": "ui-leaflet",
  "author": "https://github.com/angular-ui/ui-leaflet/graphs/contributors",
  "description": "ui-leaflet - An AngularJS directive to easily interact with Leaflet maps",
  "version": "1.0.3",
  "homepage": "http://angular-ui.github.io/ui-leaflet/",
  "keywords": [
    "angularjs",
    "javascript",
    "directive",
    "leaflet",
    "angular-ui"
